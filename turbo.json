{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "install-immutable": {
      "dependsOn": ["^install-immutable"]
    },
    "export-dynamic": {
      "cache": false,
      "dependsOn": ["tsc"]
    },
    "package-dynamic": {
      "cache": false,
      "dependsOn": ["clean-build", "install-immutable", "tsc"]
    },
    "clean-build": {
      "dependsOn": []
    },
    "build": {
      "cache": false,
      "outputs": ["dist/**"],
      "dependsOn": ["^build", "tsc"]
    },
    "tsc": {
      "outputs": ["../../dist-types/**"],
      "cache": false,
      "dependsOn": ["^tsc"]
    }
  }
}
